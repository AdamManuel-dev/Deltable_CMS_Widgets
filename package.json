    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/facebook-js-sdk": "^3.3.1",
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "facebook-js-sdk": "^0.3.4",
    "json-query": "^2.2.2",
    "node-sass": "^5.0.0",
    "nollup": "^0.13.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.2",
    "rollup": "^2.3.4",
    "rollup-plugin-hot": "^0.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte-hot": "^0.13.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sass": "^1.30.0",
    "svelte": "^3.0.0",
    "svelte-dnd-action": "^0.6.25",
    "svelte-materialify": "^0.3.4",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev:rollup": "rollup -c -w",
    "dev:livereload": "rollup -c -w --environment LIVERELOAD",
    "dev:nollup": "nollup -c --hot --port 5000 --content-base ./public",
    "dev": "run-s fix:material dev:nollup",
    "start": "sirv public",
    "fix:material": "cp ./.replacements/_variables.scss node_modules/svelte-materialify/src/styles/_variables.scss"
  }
